import java.util.Scanner;
public class LibroCalificaciones1{

    public static void main(String[] args){
        Scanner calificaciones = new Scanner(System.in);

        System.out.println("Cual es la cantidad de estudiantes a digitar");
        int cantidadDeEstudiantes = calificaciones.nextInt();
        
         int elegir;
        String nombre [] = new String [cantidadDeEstudiantes];
        int nota [] = new int [cantidadDeEstudiantes];

        for(int i = 0; i < cantidadDeEstudiantes ; i++){

            System.out.println("Cual es el nota del estudiante" + (i+1) +" : ");
            nota[i]=calificaciones.nextInt();
        }

        for(int i = 0; i<cantidadDeEstudiantes ; i++){
            System.out.println("El numero del estudiantes es " +  (i+1) + " La nota del estudiante es " + nota[i]);
            if(nota[i]>60 && nota[i]<69){
                System.out.println("El estudiante " +  (i+1) + " Esta paso con una nota regular con una nota de " + nota[i]);
                 
                 }

        }
        int reprobado=0;
        int regular=0;
        int muyBueno=0;
        int bueno=0 ;
        int excelente=0;
        int n =0;
 
         for(int i = 0; i<cantidadDeEstudiantes ; i++){
         if(nota[i]>0 && nota[i]<59){
                System.out.println("El estudiante " + (1+1) + " Esta reprobado con una nota de " + nota[i]);
                
                reprobado++;
                reprobado +=n;
         }
        }
         for(int i = 0; i<cantidadDeEstudiantes ; i++){
            if(nota[i]>60 && nota[i]<69){
                System.out.println("El estudiante " + (i+1) + " Esta paso con una nota regular con una nota de " + nota[i]);
                 regular++;
                 
         }}
                 
               for(int i = 0; i<cantidadDeEstudiantes ; i++){
               if(nota[i]>70 && nota[i]<79){
                System.out.println("El estudiante " + (i+1) + " Esta paso con una nota bueno con una nota de " + nota[i]);
                 bueno++;
                }
            }

            for(int i = 0; i<cantidadDeEstudiantes ; i++){
                 if(nota[i]>80 && nota[i]<89){
                System.out.println("El estudiante " +  (i+1) + " Esta paso con una nota muy bueno  con una nota de " + nota[i]);
                muyBueno++;
                }
            }
                 for(int i = 0; i<cantidadDeEstudiantes ; i++){
                  if(nota[i]>90 && nota[i]<100){
                System.out.println("El estudiante " +  (i+1) + " Esta paso con una nota excelente con una nota de " + nota[i]);
                System.out.println("Felicitaciones sos una maquina ");
                 excelente++;
            }

            
            if (nota [i]>60 && nota[i]<100){
                System.out.println("lOS ESTUDIANTES" +  (i+1) + " PASARON CON UNA NOTA DE :" + nota[i] );

            }
            
            
            System.out.println("");

        }
        
            for(int i = 0 ; i<n ; i ++){
                System.out.print((i+1)+"*");
            System.out.println("Total de alumnos reprobados" + reprobado);
            }
             System.out.println("Total de alumnos regulares" + regular);
              System.out.println("Total de alumnos buenos" + bueno);
               System.out.println("Total de alumnos muy buenos" + muyBueno);
                System.out.println("Total de alumnos excelente " + excelente);
    
                for(int i = 0 ; i <cantidadDeEstudiantes; i ++){
            if (nota [i]>60 && nota[i]<100){
                System.out.println("lOS ESTUDIANTES" +  (i+1)+ " PASARON CON UNA NOTA DE :" + nota[i] );

            }
                
            

       
}
}}
         
